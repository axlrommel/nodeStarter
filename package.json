{
  "name": "node-starter",
  "version": "1.0.0",
  "description": "Nodejs starter project",
  "main": "babelwrap.js",
  "scripts": {
    "dev": "yarn install --flat --force && watch --wait 1 'npm run test && npm run lint && npm run stopExpress && node src/babelwrap.js' src",
    "dependencyUpdate": "rm -rf node_modules && yarn install --flat",
    "lint": "eslint src",
    "lint:watch": "watch --wait 1 'eslint src'",
    "stopExpress": "pkill NodeJSStarter | echo 0",
    "test": "export NODE_PATH=$NODE_PATH:./src && mocha --require babel-polyfill --compilers js:babel-core/register \"src/TestStartup_spec.js\" \"src/**/*_spec.js\"",
    "start": "NODE_ENV=production node babelwrap",
    "startLocal": "NODE_ENV=local node babelwrap",
    "build:forDeploy": "npm run lint && npm run test && npm run clean:dist && npm run mkdir:dist && npm run build:server && cd dist && npm install --production",
    "build:server": "cp package.json dist & cp cf-deploy.js dist & cp -R src/* dist/ && npm run build:cleanSpecs",
    "build:cleanSpecs": "find dist/ -depth -print | grep -i _spec | xargs rm",
    "clean:dist": "rm -rf dist/*",
    "mkdir:dist": "mkdir -p dist",
    "coverage": "export NODE_PATH=$NODE_PATH:./src && istanbul cover _mocha -- -R tap --compilers js:babel-core/register --require babel-polyfill \"src/TestStartup_spec.js\" \"src/**/*_spec.js\"",
    "report": "istanbul report clover",
    "ci": "npm run coverage>test.tap && npm run report && eslint src -f checkstyle>checkstyle-result.xml"
  },
  "engines": {
    "node": "7.10.1",
    "npm": "4.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "keywords": [
    "nodejs"
  ],
  "author": "rommelvillagomez",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.209.0",
    "avsc": "^5.0.3",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.23.0",
    "babel-runtime": "^6.22.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chai-spies": "^0.7.1",
    "dotenv": "^4.0.0",
    "es6-promise": "^3.3.1",
    "eslint": "^3.16.0",
    "express": "^4.14.1",
    "fetch-mock": "^5.1.3",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^2.5.3",
    "mocha-loader": "^0.7.1",
    "rambda": "^0.9.0",
    "uuid": "^3.1.0",
    "watch": "^1.0.2"
  },
  "resolutions": {
    "commander": "2.11.0",
    "glob": "7.1.2",
    "source-map": "0.5.7",
    "minimatch": "3.0.4",
    "mkdirp": "0.5.1",
    "minimist": "1.2.0",
    "user-home": "1.1.1",
    "kind-of": "3.2.2",
    "is-number": "2.1.0",
    "isarray": "1.0.0",
    "nopt": "4.0.1",
    "string-width": "2.1.1",
    "strip-ansi": "3.0.1",
    "is-fullwidth-code-point": "2.0.0",
    "ansi-regex": "2.1.1",
    "form-data": "2.1.4",
    "qs": "6.5.0",
    "assert-plus": "0.2.0",
    "debug": "2.6.8",
    "ms": "0.7.1",
    "escape-string-regexp": "1.0.5",
    "supports-color": "3.2.3",
    "jsesc": "1.3.0",
    "regenerator-runtime": "0.11.0",
    "type-detect": "0.1.1",
    "mime": "1.3.4",
    "async": "2.5.0",
    "strip-bom": "3.0.0",
    "acorn": "5.1.1",
    "wordwrap": "1.0.0",
    "path-to-regexp": "1.7.0"
  }
}
